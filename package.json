{
  "name": "@rester/core",
  "version": "0.15.22",
  "description": "A TypeScript framework, like Spring Boot.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "restful",
    "server",
    "di",
    "spring mvc",
    "spring boot",
    "typescript",
    "nodejs"
  ],
  "homepage": "https://github.com/DevinDon/rester-core",
  "scripts": {
    "build": "./scripts/build",
    "clean": "./scripts/clean",
    "debug": "./scripts/debug",
    "init": "./scripts/init",
    "lint": "./scripts/lint",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "./scripts/push",
    "start": "npm run start:dev",
    "start:dev": "MODE=DEV ./scripts/start",
    "start:prod": "MODE=PROD ./scripts/start",
    "test": "./scripts/test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DevinDon/rester-core.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/rester-core/issues"
  },
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "mime-types": "2.1.30",
    "reflect-metadata": "0.1.13",
    "yaml": "2.0.0-4"
  },
  "devDependencies": {
    "@rester/logger": "0.2.2",
    "@rester/orm": "0.1.12",
    "@types/jasmine": "3.6.11",
    "@types/mime-types": "2.1.0",
    "@types/mongodb": "3.6.12",
    "@types/node": "15.0.2",
    "@types/yaml": "1.9.7",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "class-validator": "0.13.1",
    "eslint": "7.25.0",
    "jasmine": "3.7.0",
    "jasmine-console-reporter": "3.1.0",
    "mongodb": "3.6.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
