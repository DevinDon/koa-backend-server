{
  "name": "@rester/core",
  "version": "0.12.10",
  "description": "A TypeScript framework, like Spring Boot.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "restful",
    "server",
    "di",
    "spring mvc",
    "spring boot",
    "typescript",
    "nodejs"
  ],
  "homepage": "https://github.com/DevinDon/rester-core",
  "scripts": {
    "build": "npm run build:dist & npm run build:demo & npm run build:main & npm run build:test",
    "build:dist": "tsc",
    "build:demo": "cd src/demo && tsc",
    "build:main": "cd src/main && tsc",
    "build:test": "cd src/test && tsc",
    "clean": "sh -c \"rm -rf dist out\"",
    "demo:full": "node -r ts-node/register src/demo/full",
    "demo:simple": "node -r ts-node/register src/demo/simple",
    "prepublishOnly": "npm run clean && npm run build:dist",
    "publish": "curl https://npm.taobao.org/sync/@rester/core",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DevinDon/rester-core.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/rester-core/issues"
  },
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@iinfinity/delay": "0.1.1",
    "@iinfinity/logger": "0.3.4",
    "mongodb": "^3.6.2",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.28"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.14",
    "@types/node": "14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "jasmine": "3.6.1",
    "jasmine-console-reporter": "3.1.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "optionalDependencies": {}
}
