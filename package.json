{
  "name": "@iinfinity/rester",
  "version": "0.8.0",
  "description": "A RESTful server with DI, DB & session support based Fastify & TypeScript.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "restful",
    "backend",
    "server"
  ],
  "homepage": "https://github.com/DevinDon/rester",
  "scripts": {
    "build": "tsc",
    "demo:full": "node -r ts-node/register src/demo/full",
    "demo:simple": "node -r ts-node/register src/demo/simple",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DevinDon/rester.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/rester/issues"
  },
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@iinfinity/delay": "^0.1.1",
    "@iinfinity/jwt": "^0.1.1",
    "@iinfinity/logger": "^0.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "@types/node": "^12.0.3",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "optionalDependencies": {}
}
